{
  "_comment": "OpenWebUI configuration template - sanitized for version control",
  "_usage": "Import via Admin Settings > Export/Import or apply with bootstrap.sh",

  "ui": {
    "default_locale": "en-US",
    "show_username": true,
    "chat_bubble_ui": false,
    "wide_screen_mode": true
  },

  "model_defaults": {
    "_comment": "Applied to all models unless overridden",
    "temperature": 0.7,
    "top_p": 0.9,
    "top_k": 40,
    "repeat_penalty": 1.1,
    "num_ctx": 8192,
    "num_predict": -1,
    "stop": []
  },

  "model_configs": {
    "_comment": "See config/open-webui-model-presets.json for importable presets",

    "qwen2.5-coder:14b": {
      "name": "Qwen 2.5 Coder 14B",
      "description": "Primary coding model - GPU accelerated (RTX 5080)",
      "params": {
        "temperature": 0.3,
        "top_p": 0.95,
        "num_ctx": 32768,
        "repeat_penalty": 1.05
      },
      "system_prompt": "You are an expert software engineer. Write clean, idiomatic, production-ready code. Explicit error handling, type hints in Python, strict mode in shell scripts."
    },

    "phi4:latest": {
      "name": "Phi-4 14B",
      "description": "Reasoning and analysis - GPU accelerated (RTX 5080)",
      "params": {
        "temperature": 0.5,
        "top_p": 0.9,
        "num_ctx": 16384,
        "repeat_penalty": 1.1
      },
      "system_prompt": "You are a thoughtful assistant that excels at reasoning through complex problems step by step."
    },

    "llama3.1:8b": {
      "name": "Llama 3.1 8B",
      "description": "Fast general-purpose - GPU accelerated (RTX 5080)",
      "params": {
        "temperature": 0.7,
        "top_p": 0.9,
        "num_ctx": 8192
      },
      "system_prompt": "You are a helpful, concise assistant. Prioritize speed and clarity."
    },

    "llava:13b": {
      "name": "LLaVA 13B",
      "description": "Vision/multimodal - GPU accelerated (RTX 5080)",
      "params": {
        "temperature": 0.4,
        "top_p": 0.9,
        "num_ctx": 4096,
        "repeat_penalty": 1.1
      },
      "system_prompt": "You are a multimodal AI assistant that can analyze both text and images. Be precise about what you see."
    },

    "mistral:7b": {
      "name": "Mistral 7B",
      "description": "General chat - CPU fallback (available when GPU is busy)",
      "params": {
        "temperature": 0.7,
        "top_p": 0.9,
        "num_ctx": 8192
      },
      "system_prompt": "You are a helpful assistant running on CPU. Keep responses concise and focused."
    },

    "nomic-embed-text:latest": {
      "name": "Nomic Embed Text",
      "description": "Text embeddings for RAG (CPU)",
      "params": {},
      "system_prompt": ""
    }
  },

  "workspace": {
    "default_model": "qwen2.5-coder:14b",
    "task_model": "llama3.1:8b",
    "embedding_model": "nomic-embed-text:latest",
    "default_prompt_suggestions": [
      "Review this code for bugs and suggest improvements",
      "Explain this function and suggest better naming",
      "Write unit tests for this module",
      "Analyze this image and describe what you see",
      "Break down this problem step by step"
    ]
  },

  "admin": {
    "_comment": "Settings that require admin access to apply",
    "enable_signup": true,
    "enable_community_sharing": false,
    "default_user_role": "user"
  }
}
