{
  "_meta": {
    "name": "FLUX.1 Schnell - Fast High Quality",
    "description": "Fast high-quality generation using FLUX.1 Schnell model. Excellent prompt following.",
    "version": "1.0.0",
    "author": "self-hosted-ai",
    "tags": ["txt2img", "flux", "fast", "high-quality"],
    "requirements": {
      "checkpoint": "flux1-schnell.safetensors",
      "vram_min_gb": 12,
      "custom_node": "ComfyUI-FLUX"
    }
  },
  "1": {
    "inputs": {
      "unet_name": "flux1-schnell.safetensors",
      "weight_dtype": "default"
    },
    "class_type": "UNETLoader",
    "_meta": { "title": "Load FLUX UNET" }
  },
  "2": {
    "inputs": {
      "clip_name1": "t5xxl_fp16.safetensors",
      "clip_name2": "clip_l.safetensors",
      "type": "flux"
    },
    "class_type": "DualCLIPLoader",
    "_meta": { "title": "Load Dual CLIP" }
  },
  "3": {
    "inputs": {
      "vae_name": "ae.safetensors"
    },
    "class_type": "VAELoader",
    "_meta": { "title": "Load VAE" }
  },
  "4": {
    "inputs": {
      "width": 1024,
      "height": 1024,
      "batch_size": 1
    },
    "class_type": "EmptyLatentImage",
    "_meta": { "title": "Empty Latent Image" }
  },
  "5": {
    "inputs": {
      "text": "Prompt",
      "clip": ["2", 0]
    },
    "class_type": "CLIPTextEncode",
    "_meta": { "title": "CLIP Text Encode" }
  },
  "6": {
    "inputs": {
      "guidance": 3.5,
      "conditioning": ["5", 0]
    },
    "class_type": "FluxGuidance",
    "_meta": { "title": "FLUX Guidance" }
  },
  "7": {
    "inputs": {
      "noise": ["8", 0],
      "guider": ["9", 0],
      "sampler": ["10", 0],
      "sigmas": ["11", 0],
      "latent_image": ["4", 0]
    },
    "class_type": "SamplerCustomAdvanced",
    "_meta": { "title": "Sampler Custom Advanced" }
  },
  "8": {
    "inputs": {
      "noise_seed": 0
    },
    "class_type": "RandomNoise",
    "_meta": { "title": "Random Noise" }
  },
  "9": {
    "inputs": {
      "model": ["1", 0],
      "conditioning": ["6", 0]
    },
    "class_type": "BasicGuider",
    "_meta": { "title": "Basic Guider" }
  },
  "10": {
    "inputs": {
      "sampler_name": "euler"
    },
    "class_type": "KSamplerSelect",
    "_meta": { "title": "KSampler Select" }
  },
  "11": {
    "inputs": {
      "model": ["1", 0],
      "scheduler": "simple",
      "steps": 4,
      "denoise": 1
    },
    "class_type": "BasicScheduler",
    "_meta": { "title": "Basic Scheduler (4 Steps)" }
  },
  "12": {
    "inputs": {
      "samples": ["7", 0],
      "vae": ["3", 0]
    },
    "class_type": "VAEDecode",
    "_meta": { "title": "VAE Decode" }
  },
  "13": {
    "inputs": {
      "filename_prefix": "FLUX_schnell",
      "images": ["12", 0]
    },
    "class_type": "SaveImage",
    "_meta": { "title": "Save Image" }
  }
}
